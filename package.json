{
  "name": "nebutra-sailor",
  "version": "1.0.0",
  "private": true,
  "description": "Enterprise-grade SaaS monorepo for AI-native, multi-tenant platforms",
  "scripts": {
    "dev": "turbo run dev",
    "build": "pnpm brand:sync && turbo run build",
    "lint": "turbo run lint",
    "typecheck": "turbo run typecheck",
    "test": "turbo run test",
    "test:coverage": "turbo run test -- --coverage",
    "check-env": "npx tsx infra/scripts/check-env.ts",
    "db:generate": "turbo run db:generate",
    "db:migrate": "turbo run db:migrate",
    "db:push": "turbo run db:push",
    "db:studio": "pnpm --filter @nebutra/db db:studio",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "clean": "turbo run clean && rm -rf node_modules",
    "brand:sync": "pnpm --filter @nebutra/brand sync"
  },
  "keywords": ["saas", "monorepo", "nextjs", "ai", "multi-tenant"],
  "author": "Nebutra",
  "license": "MIT",
  "packageManager": "pnpm@10.14.0",
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@types/node": "^24.10.1",
    "@vitejs/plugin-react": "^4.4.1",
    "jsdom": "^26.0.0",
    "tsx": "^4.19.2",
    "turbo": "^2.6.1",
    "typescript": "^5.9.3",
    "vitest": "^3.2.1"
  }
}
