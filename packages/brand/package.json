{
  "name": "@nebutra/brand",
  "version": "0.1.0",
  "private": true,
  "description": "Nebutra brand assets, logos, and identity components",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./logo": {
      "types": "./dist/components/Logo.d.ts",
      "import": "./dist/components/Logo.js"
    },
    "./metadata": {
      "types": "./dist/metadata.d.ts",
      "import": "./dist/metadata.js"
    },
    "./assets/*": "./assets/*"
  },
  "files": [
    "dist",
    "assets"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "sync": "tsx scripts/sync-assets.ts"
  },
  "dependencies": {
    "react": "^19.2.1"
  },
  "devDependencies": {
    "@types/react": "^19.0.0",
    "tsup": "^8.0.0",
    "tsx": "^4.19.0",
    "typescript": "^5.6.0"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0"
  }
}
