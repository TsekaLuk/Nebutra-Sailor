{
  "name": "@nebutra/custom-ui",
  "version": "0.1.0",
  "private": true,
  "description": "Brand-specific UI components built on top of @nebutra/design-system - domain-specific, complex, and high-density UI",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./primitives": {
      "types": "./dist/primitives/index.d.ts",
      "import": "./dist/primitives/index.js"
    },
    "./primitives/*": {
      "types": "./dist/primitives/*.d.ts",
      "import": "./dist/primitives/*.js"
    },
    "./components/*": {
      "types": "./dist/components/*.d.ts",
      "import": "./dist/components/*.js"
    },
    "./layouts/*": {
      "types": "./dist/layouts/*.d.ts",
      "import": "./dist/layouts/*.js"
    },
    "./widgets/*": {
      "types": "./dist/widgets/*.d.ts",
      "import": "./dist/widgets/*.js"
    },
    "./marketing": {
      "types": "./dist/marketing/index.d.ts",
      "import": "./dist/marketing/index.js"
    },
    "./marketing/*": {
      "types": "./dist/marketing/*.d.ts",
      "import": "./dist/marketing/*.js"
    },
    "./styles/globals.css": "./src/styles/globals.css",
    "./tailwind.preset": {
      "types": "./dist/tailwind.preset.d.ts",
      "import": "./dist/tailwind.preset.js"
    },
    "./lib/utils": {
      "types": "./dist/utils/cn.d.ts",
      "import": "./dist/utils/cn.js"
    },
    "./patterns": {
      "types": "./dist/patterns/index.d.ts",
      "import": "./dist/patterns/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@heroui/calendar": "^2.2.27",
    "@heroui/card": "^2.2.25",
    "@heroui/checkbox": "^2.3.27",
    "@heroui/date-input": "^2.3.27",
    "@heroui/popover": "^2.3.18",
    "@heroui/progress": "^2.2.22",
    "@heroui/select": "^2.4.16",
    "@heroui/skeleton": "^2.2.10",
    "@heroui/switch": "^2.2.24",
    "@internationalized/date": "^3.10.0",
    "@nebutra/design-system": "workspace:*",
    "@paper-design/shaders-react": "^0.0.67",
    "@radix-ui/react-accordion": "^1.2.0",
    "@radix-ui/react-avatar": "^1.1.0",
    "@radix-ui/react-dialog": "^1.1.0",
    "@radix-ui/react-dropdown-menu": "^2.1.0",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-slot": "^1.2.0",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.1.0",
    "canvas-confetti": "^1.9.4",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "cobe": "^0.6.3",
    "date-fns": "^4.1.0",
    "dotted-map": "^2.2.2",
    "embla-carousel-react": "^8.6.0",
    "framer-motion": "^11.0.0",
    "input-otp": "^1.4.2",
    "lucide-react": "^0.460.0",
    "motion": "^12.23.25",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-syntax-highlighter": "^15.6.1",
    "react-tweet": "^3.2.0",
    "react-use-measure": "^2.1.7",
    "recharts": "^2.15.0",
    "rough-notation": "^0.5.1",
    "svg-dotted-map": "^2.0.1",
    "tailwind-merge": "^2.5.0",
    "usehooks-ts": "^3.1.0",
    "vaul": "^1.1.2"
  },
  "optionalDependencies": {
    "@react-three/fiber": "^8.0.0",
    "three": "^0.170.0"
  },
  "devDependencies": {
    "@types/canvas-confetti": "^1.9.0",
    "@types/node": "^20.14.0",
    "@types/react": "^19.0.0",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@types/recharts": "^2.0.1",
    "@types/three": "^0.170.0",
    "next": "^16.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.6.0"
  },
  "peerDependencies": {
    "@clerk/nextjs": ">=5.0.0",
    "next": ">=14.0.0",
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "peerDependenciesMeta": {
    "@clerk/nextjs": {
      "optional": true
    },
    "next": {
      "optional": true
    }
  }
}
