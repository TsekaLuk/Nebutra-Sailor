{
  "name": "@nebutra/legal",
  "version": "0.1.0",
  "private": true,
  "description": "Legal & Compliance infrastructure for Nebutra SaaS",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/types/index.js"
    },
    "./documents": {
      "types": "./dist/documents/index.d.ts",
      "import": "./dist/documents/index.js"
    },
    "./consent": {
      "types": "./dist/consent/index.d.ts",
      "import": "./dist/consent/index.js"
    },
    "./components": {
      "types": "./dist/components/index.d.ts",
      "import": "./dist/components/index.js"
    },
    "./content/*": "./content/*"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "lint": "tsc --noEmit"
  },
  "dependencies": {
    "@nebutra/db": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/react": "^19.0.0",
    "react": "^19.2.1",
    "tsup": "^8.3.5",
    "typescript": "^5.6.0"
  },
  "peerDependencies": {
    "react": "^19.0.0"
  }
}
